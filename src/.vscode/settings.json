{
    "files.exclude": {
        "**/**/.cache": true,
        "**/**/node_modules": true,
        "**/**/package-lock.json": true,
        "**/*.d.ts": { "when": "$(basename).js" },
        "**/*.js": { "when": "$(basename).ts" },
        "**/*.js.map": true,
        "**/.vs": true,
        "**/bin": true,
        "**/obj": true,
        "**/.vscode": true,
    }
}
